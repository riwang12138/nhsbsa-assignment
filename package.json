{
  "name": "nhsbsa-assignment",
  "version": "0.0.1",
  "description": "Assignment project for nhsbsa developer",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/start",
    "test": "jest",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ignore-path ./.gitignore . ",
    "lint:prettier": "prettier --check --ignore-path ./.gitignore **/*.{js,jsx} ",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --fix --ignore-path .gitignore . ",
    "format:prettier": "prettier --write --ignore-path .gitignore **/*.{js,jsx} "
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.2",
    "express-validator": "^6.12.0",
    "govuk-frontend": "^3.12.0",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "node-sass-middleware": "^0.11.0",
    "nunjucks": "^3.2.3"
  },
  "devDependencies": {
    "jest": "^27.0.4",
    "supertest": "^6.1.3",
    "eslint": "^7.26.0",
    "prettier": "^2.0.4",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
