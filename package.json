{
  "name": "nhsbsa-assignment",
  "version": "0.0.1",
  "description": "Assignment project for nhsbsa developer",
  "private": true,
  "main": "app.js",
  "scripts": {
    "eslint": "./node_modules/eslint/bin/eslint.js --max-warnings 0 --ext .js .",
    "start": "node ./bin/start",
    "test": "./node_modules/mocha/bin/mocha --recursive",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ignore-path ./.gitignore . ",
    "lint:prettier": "prettier --check --ignore-path ./.gitignore **/*.{js,jsx} ",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --fix --ignore-path .gitignore . ",
    "format:prettier": "prettier --write --ignore-path .gitignore **/*.{js,jsx} "
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "nunjucks": "^3.2.3",
    "node-sass-middleware": "^0.11.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.26.0",
    "prettier": "^2.0.4",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mocha": "^8.4.0"
  }
}
